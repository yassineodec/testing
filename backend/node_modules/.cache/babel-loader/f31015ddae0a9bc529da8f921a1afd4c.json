this.message.notiError = \"El nombre de usuario ya existe\";\n              this.message.notiSuccess = \"\";\n            } else {\n            }\n          })\n          .catch((err) => {\n            // console.log(err);\n          });\n      } else {\n        this.message.notiError = \"Error! Invalid Forum ðŸ˜¢\";\n        this.message.notiSuccess = \"\";\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.text-vs {\n  font-size: 0.7rem;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}