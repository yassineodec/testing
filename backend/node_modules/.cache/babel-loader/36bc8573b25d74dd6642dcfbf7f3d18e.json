{"ast":null,"code":"export default {\n  data() {\n    return {\n      email: '',\n      message: []\n    };\n  },\n\n  watch: {\n    email(val) {\n      if (val.length > 2) {\n        if (/^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$/.test(val)) {\n          this.email = val;\n          this.message['email'] = '';\n        } else {\n          // this.email = ''\n          this.message['email'] = '* email is not valid';\n        }\n      } else if (val.length < 1) {\n        // this.email = ''\n        this.message['email'] = '';\n      }\n    }\n\n  },\n  methods: {\n    submitEmail() {\n      if (!this.email == '') {\n        alert(`Email : ${this.email}`);\n        this.message.notiError = false;\n        this.message.notiSuccess = true;\n      } else {\n        this.message.notiError = true;\n        this.message.notiSuccess = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqDA,eAAe;AACXA,MAAI,GAAG;AACH,WAAO;AACHC,WAAK,EAAE,EADJ;AAEHC,aAAO,EAAE;AAFN,KAAP;AAIH,GANU;;AAOXC,OAAK,EAAE;AACHF,SAAK,CAACG,GAAD,EAAM;AACP,UAAIA,GAAG,CAACC,MAAJ,GAAa,CAAjB,EAAoB;AAChB,YAAI,gDAAgDC,IAAhD,CAAqDF,GAArD,CAAJ,EAA+D;AAC3D,eAAKH,KAAL,GAAaG,GAAb;AACA,eAAKF,OAAL,CAAa,OAAb,IAAwB,EAAxB;AACJ,SAHA,MAGO;AACH;AACA,eAAKA,OAAL,CAAa,OAAb,IAAwB,sBAAxB;AACJ;AACJ,OARA,MAQO,IAAIE,GAAG,CAACC,MAAJ,GAAa,CAAjB,EAAoB;AACvB;AACA,aAAKH,OAAL,CAAa,OAAb,IAAwB,EAAxB;AACJ;AACH;;AAdE,GAPI;AAuBXK,SAAO,EAAE;AACLC,eAAW,GAAG;AACV,UAAI,CAAC,KAAKP,KAAN,IAAe,EAAnB,EAAuB;AACnBQ,aAAK,CAAE,WAAU,KAAKR,KAAM,EAAvB,CAAL;AACA,aAAKC,OAAL,CAAaQ,SAAb,GAAyB,KAAzB;AACA,aAAKR,OAAL,CAAaS,WAAb,GAA2B,IAA3B;AACJ,OAJA,MAIO;AACH,aAAKT,OAAL,CAAaQ,SAAb,GAAyB,IAAzB;AACA,aAAKR,OAAL,CAAaS,WAAb,GAA2B,KAA3B;AACJ;AACJ;;AAVK;AAvBE,CAAf","names":["data","email","message","watch","val","length","test","methods","submitEmail","alert","notiError","notiSuccess"],"sourceRoot":"","sources":["C:\\Users\\yassine\\Desktop\\Curso Angular\\toolsapp-vue-master\\src\\views\\auth\\resetPass.vue"],"sourcesContent":["<template>\n<di