{"ast":null,"code":"export default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      message: []\n    };\n  },\n\n  watch: {\n    email(val) {\n      if (val.length > 2) {\n        if (/^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$/.test(val)) {\n          this.email = val;\n          this.message[\"email\"] = \"\";\n        } else {\n          // this.email = ''\n          this.message[\"email\"] = \"* email is not valid\";\n        }\n      } else if (val.length < 1) {\n        // this.email = ''\n        this.message[\"email\"] = \"\";\n      }\n    }\n\n  },\n  methods: {\n    submitLogin() {\n      if (!this.email == \"\" && !this.password == \"\" && this.password.length >= 8) {\n        this.axios.post(\"http://127.0.0.1:10520/api/user/login\", {\n          email: this.email,\n          password: this.password\n        }).then(res => {\n          if (res.data.status_code == 200) {\n            // console.log(res.data)\n            this.message.notiError = \"\";\n            this.message.notiSuccess = \"Login Successfuly ðŸŽ‰ Redirecting...\";\n            window.sessionStorage.setItem(\"access_token\", res.data.token);\n            this.$store.state.hasToken = true;\n            this.$store.state.user = res.data.user;\n            setTimeout(this.redirect, 2000);\n          } else {\n            this.message.notiError = `${res.data.message} ðŸ˜¢`;\n            this.message.notiSuccess = \"\";\n          }\n        }).catch(err => {\n          // console.log(err);\n          this.message.notiError = \"Email not found ðŸ˜¢\";\n          this.message.notiSuccess = \"\";\n        });\n      } else {\n        this.message.notiError = \"Error! Invalid Forum ðŸ˜¢\";\n        this.message.notiSuccess = \"\";\n      }\n    },\n\n    redirect() {\n      this.$router.push(\"/\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4HA,eAAe;AACbA,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,EADF;AAELC,cAAQ,EAAE,EAFL;AAGLC,aAAO,EAAE;AAHJ,KAAP;AAK